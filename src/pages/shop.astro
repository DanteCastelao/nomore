---
import ShopMobile from "../components/ShopMobile.astro";
import Layout from "../layouts/Layout.astro";

if (Astro.request.method === "POST") {
    const body = await Astro.request.json();
    const { email } = body;
    try {
        await fetch("https://nomore-back-end.onrender.com/emails", {
            method: "POST",
            headers: {
                "Content-Type": "application/json",
            },
            body: JSON.stringify({ email }),
        });
        return {
            statusCode: 200,
            body: "Success",
        };
    } catch (error) {
        return {
            statusCode: 500,
            body: "Error",
        };
    }
}
---

<Layout title="nomore-shop">
    <div class="container">
		<div class="gradient-top"></div>
        <div class="gradient-bottom"></div>
		<div class="logo-container">
			<div class="logo">
				<span>nomore</span>
				<div class="justified">
					<span>n</span>
					<span>m</span>
					<span>re</span>
				</div>
				<span>nm</span>
				<div class="last-word">
					<span>n</span>
					<span>m</span>
				</div>
			</div>
		</div>
        <div class="form">
            <span>DROP1</span>
            <span>COMING SOON</span>
            <svg width="29" height="18" viewBox="0 0 29 18" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
                <rect width="29" height="18" fill="url(#pattern0)"/>
                <defs>
                <pattern id="pattern0" patternContentUnits="objectBoundingBox" width="1" height="1">
                <use xlink:href="#image0_187_285" transform="matrix(0.000833333 0 0 0.00134259 0 -0.00347222)"/>
                </pattern>
                <image id="image0_187_285" width="1200" height="750" xlink:href="data:image/png;base64,"/>
                </defs>
            </svg>                
            <span>DEJA TU MAIL PARA SER NOTIFICADO</span>
            <form class="input" method="POST">
                <input type="email" name="email"/>
                <button type="submit">&#62;</button>
            </form>
            <div class="links">
                <a href="/prtyps">PRTYPS</a>
                <a href="https://www.instagram.com/no.more.tm?igsh=eHd3eXJzMmVhczlj">INSTAGRAM</a>
            </div>
        </div>
        <video class="video" src="teaser.mp4" autoplay muted loop></video>
	</div>
    <ShopMobile />
</Layout>

<script>
    const element = document.body;
  
    const savedMode = document.cookie.match('theme=light');
  
    if (savedMode) {
      element.classList.add('light-mode');
    } else {
      element.classList.remove('light-mode');
    }
</script>

<style>
    a{
        color : #f5f5f5;
    }
	.container {
		display: flex;
		flex-direction: row;
		height: 100%;
		padding-left: 6%;
        color: #f5f5f5;
	}
    .video {
        position: absolute;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        object-fit: cover;
        z-index: -1;
    }
	.gradient-top {
        position: absolute;
        top: 0;
        left: 0;
        width: 100%;
        height: 20%;
        background: linear-gradient(to bottom, #030303 0%, rgba(0, 0, 0, 0) 100%);
    }
    .gradient-bottom {
        position: absolute;
        bottom: 0;
        left: 0;
        width: 100%;
        height: 20%;
        background: linear-gradient(to top, #030303 0%, rgba(0, 0, 0, 0) 100%);
    }
	.logo-container {
		display: flex;
		margin-top: 2rem;
		user-select: none;
	}
	.logo {
		line-height: 10rem;
		display: flex;
		flex-direction: column;
	}
	.logo span {
		font-size: clamp(1rem, 10vw, 11vw);
	}
	.justified {
		display: flex;
		justify-content: space-between;
	}
	.last-word {
		display: flex;
		flex-direction: column;
		line-height: 8rem;
	}
    .form {
        position: absolute;
        top: 50%;
        left: 50%;
        transform: translate(-50%, -50%);
        flex-direction: column;
        display: flex;
        text-align: center;
        align-items: center;
        gap: 1rem;
    }
    @media screen and (max-width: 800px) {
		.container {
			display: none
		}
	}
    .input input {
        background: none;
        border: none;
        border-bottom: 2px solid #f5f5f5;
        color: #f5f5f5;
        font-size: 1rem;
    }
    textarea:focus, input:focus{
        outline: none;
    }
    .input button {
        background: none;
        border: none;
        color: #f5f5f5;
        cursor: pointer;
        text-decoration: underline;
        text-decoration-thickness: 2px;
        font-size: 1.2rem;
    }
    .links {
        display: flex;
        flex-direction: row;
        gap: 1rem;
        margin-top: 10rem;
        color: #f5f5f5;
    }
</style>